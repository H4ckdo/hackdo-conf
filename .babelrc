{
  "plugins": [
    "transform-decorators-legacy",
    "transform-runtime",
    "transform-object-rest-spread",
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "anchors": "../config/anchors",
          "stores": "./client/src/js/stores",
          "components": "./client/src/js/components",
          "config":"../config",
          "utils":"../utils"
        }
      }
    ]
  ],  
  "presets": [
    "es2015",
    "react",
    "stage-2"
  ]
}